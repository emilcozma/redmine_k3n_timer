<h2><%= l(:label_k3n_timer) %></h2>

<%= labelled_form_for @office_time_entry, :url => {:controller => 'k3n_timer', :action => 'create'}, :html => {:multipart => true} do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= submit_tag l(:button_create) %>
  <%= submit_tag l(:button_create_and_continue), :name => 'continue' %>
<% end %>
